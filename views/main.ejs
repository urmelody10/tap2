        .set{
            text-align: center;

          }

          .lineUp {
                  animation: 2s anim-lineUp ease-out infinite;
                }
                @keyframes anim-lineUp {
                  0% {
                    opacity: 0;
                    transform: translateY(80%);
                  }
                  20% {
                    opacity: 0;
                  }
                  50% {
                    opacity: 1;
                    transform: translateY(0%);
                  }
                  100% {
                    opacity: 1;
                    transform: translateY(0%);
                  }
                }

          body {
            font-family: 'Rajdhani', sans-serif;
          }

            ul {
          list-style-type: none;
          margin: 10;
          padding: 20;
          overflow: hidden;
          background-color: #0f1554;
        }

        li.customfont { 
          font-family: 'Rajdhani', sans-serif;
        }
        
        li {
          float: left;   
        }
        
        li a {
          border-radius: 25px;
          display: block;
          color: #ffffff;
          text-align: center;
          padding: 14px 16px;
          text-decoration: none;
          
        }
        
        li a:hover:not(.active) {
          background-color: #141a5f;
        }
        
        .active {
          background-color: #141a5f;
        }

        .what_is {
          display: grid;
          grid-template-columns: repeat(5, 1fr);
          grid-template-rows: repeat(5, 1fr);
          grid-column-gap: 0px;
          grid-row-gap: 0px;
        }

        .div1 { grid-area: 4 / 1 / 5 / 2; }
        .div2 { grid-area: 4 / 2 / 5 / 3; }

    </style>
